    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductlistforUser</servlet-name>
        <url-pattern>/productlistforUser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

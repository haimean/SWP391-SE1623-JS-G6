        <servlet-name>CategoryUpdate</servlet-name>
        <url-pattern>/admin/category/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductList</servlet-name>
        <url-pattern>/seller/product</url-pattern>
    </servlet-mapping>

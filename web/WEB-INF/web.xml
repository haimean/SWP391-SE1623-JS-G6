        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SellerProfileController</servlet-name>
        <servlet-class>controller.SellerProfileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminProfile</servlet-name>
        <servlet-class>controller.AdminProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerProfile</servlet-name>
        <servlet-class>controller.CustomerProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductlistforUser</servlet-name>
        <servlet-class>controller.ProductlistforUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Category_List</servlet-name>
        <url-pattern>/category_list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Category_Delete</servlet-name>
        <url-pattern>/category_delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Category_Search</servlet-name>
        <url-pattern>/category_search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Category_Update</servlet-name>
        <url-pattern>/category_update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Category_Create</servlet-name>
        <url-pattern>/category_create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserlistController_List</servlet-name>
        <url-pattern>/userlist_list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Userlist_Delete</servlet-name>
        <url-pattern>/userlist_delete</url-pattern>
    </servlet-mapping>

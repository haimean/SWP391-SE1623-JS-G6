    <servlet>
        <servlet-name>Category_Delete</servlet-name>
        <servlet-class>controller.Category_Delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Category_Search</servlet-name>
        <servlet-class>controller.Category_Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Category_Update</servlet-name>
        <servlet-class>controller.Category_Update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Category_Create</servlet-name>
        <servlet-class>controller.Category_Create</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserlistController_List</servlet-name>
        <servlet-class>controller.UserlistController_List</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Userlist_Delete</servlet-name>

    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SellerProfileController</servlet-name>
        <url-pattern>/sellerProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductlistforUser</servlet-name>
        <url-pattern>/productlistforUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminDashboard</servlet-name>
        <url-pattern>/admin</url-pattern>
